<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

  <!--　ヴァル研が提供する駅情報CSV を取込む際に置換したい会社名、路線名、駅名を定義する。  -->
  <bean class="jp.co.transcosmos.dm3.lookup.CodeLookupList">
    <property name="codeLookupManager"><ref bean="codeLookupManager"/></property>
    <property name="lookups">
      <map>
        <!-- 駅情報CSV の「鉄道会社名」を、決まった文字列に変換して登録したい場合、このリストに設定する。 -->
        <entry key="rrName">
          <props>
            <prop key="岡山電気軌道">岡電</prop>
            <prop key="スカイレールサービス">スカイレール</prop>
            <prop key="広島高速交通">アストラムライン</prop>
            <prop key="広島電鉄">広電</prop>
            <prop key="高松琴平電気鉄道">琴電</prop>
            <prop key="伊予鉄道">伊予鉄</prop>
            <prop key="土佐電気鉄道">土佐電鉄</prop>
            <prop key="京都市交通局">京都市営地下鉄</prop>
            <prop key="大阪高速鉄道">大阪モノレール</prop>
            <prop key="大阪市交通局">大阪市営地下鉄</prop>
            <prop key="大阪府都市開発">泉北高速鉄道</prop>
            <prop key="神戸市交通局">神戸市営地下鉄</prop>
            <prop key="北越急行">ほくほく線</prop>
            <prop key="北陸鉄道">北鉄</prop>
            <prop key="松本電気鉄道">松電</prop>
            <prop key="長野電鉄">長電</prop>
            <prop key="名古屋鉄道">名鉄</prop>
            <prop key="伊豆急行">伊豆急</prop>
            <prop key="遠州鉄道">遠鉄</prop>
            <prop key="静岡鉄道">静鉄</prop>
            <prop key="天竜浜名湖鉄道">天浜線</prop>
            <prop key="愛知環状鉄道">愛環</prop>
            <prop key="愛知高速交通">リニモ</prop>
            <prop key="近畿日本鉄道">近鉄</prop>
            <prop key="東海交通事業">城北線</prop>
            <prop key="豊橋鉄道">豊鉄</prop>
            <prop key="名古屋市交通局">地下鉄</prop>
            <prop key="名古屋臨海高速鉄道">あおなみ線</prop>
            <prop key="首都圏新都市鉄道">つくばエクスプレス</prop>
            <prop key="東京地下鉄">東京メトロ</prop>
            <prop key="山万">山万ユーカリが丘線</prop>
            <prop key="東京都交通局">都営地下鉄</prop>
            <prop key="横浜市交通局">横浜市営地下鉄</prop>
            <prop key="横浜新都市交通">金沢シーサイドライン</prop>
          </props>
        </entry>

        <!-- 駅情報CSV の「路線名」を、決まった文字列に変換して登録したい場合、このリストに設定する。 -->
        <entry key="routeName">
          <props>
            <prop key="P007001">スカイレール</prop>      <!-- スカイレールサービス -->
            <prop key="P082001">ポートライナー</prop>    <!-- ポートアイランド線 -->
            <prop key="P082003">ポートライナー</prop>    <!-- ポートアイランド線 -->
            <prop key="P082002">六甲ライナー</prop>      <!-- 六甲アイランド線 -->
            <prop key="J004056">きのくに線</prop>        <!-- 紀勢本線 -->
            <prop key="J002005">長野新幹線</prop>        <!-- 北陸新幹線 -->
            <prop key="P023002">ブルーライン</prop>      <!-- １号線 -->
            <prop key="P023001">ブルーライン</prop>      <!-- ３号線 -->
            <prop key="P023003">グリーンライン</prop>    <!-- ４号線 -->
            <!--<prop key="J002037">宇都宮線</prop>-->          <!-- 東北本線 -->
          </props>
        </entry>

        <!-- 駅情報CSV の「路線名カッコ付」を、決まった文字列に変換して登録したい場合、このリストに設定する。 -->
        <entry key="routeNameFull">
          <props>
            <prop key="P007001">スカイレール</prop>                      <!-- スカイレールサービス -->
            <prop key="P082001">ポートライナー(三宮－神戸空港)</prop>    <!-- ポートアイランド線(三宮－神戸空港) -->
            <prop key="P082003">ポートライナー(市民広場－中公園)</prop>  <!-- ポートアイランド線(市民広場－中公園) -->
            <prop key="P082002">六甲ライナー</prop>                      <!-- 六甲アイランド線 -->
            <prop key="J004056">きのくに線</prop>                        <!-- 紀勢本線(和歌山－新宮) -->
            <prop key="J002005">長野新幹線</prop>                        <!-- 北陸新幹線 -->
			<prop key="P023002">ブルーライン（関内－湘南台）</prop>      <!-- １号線 -->
			<prop key="P023001">ブルーライン（あざみ野－関内）</prop>    <!-- ３号線 -->
			<prop key="P023003">グリーンライン</prop>                    <!-- ４号線 -->
			<prop key="J002040">常磐線（快速）</prop>                            <!-- 常磐線(上野－仙台) -->
			<prop key="J002041">常磐線（各駅停車）</prop>                <!-- 常磐線(北千住－取手) -->
			<prop key="J002024">京葉線</prop>                            <!-- 京葉線(東京－蘇我) -->
			<prop key="J002013">南武線</prop>                            <!-- 南武線(川崎－立川) -->
			<prop key="P116002">丸ノ内線</prop>                          <!-- 丸ノ内線(池袋－荻窪) -->
          </props>
        </entry>

        <!-- 駅情報CSV の「路線名鉄道会社付き」を、決まった文字列に変換して登録したい場合、このリストに設定する。 -->
        <entry key="routeNameRr">
          <props>
            <prop key="P007001">スカイレール</prop>                    <!-- スカイレールサービス -->
            <prop key="P082001">神戸新交通ポートライナー</prop>        <!-- 神戸新交通ポートアイランド線 -->
            <prop key="P082003">神戸新交通ポートライナー</prop>        <!-- 神戸新交通ポートアイランド線 -->
            <prop key="P082002">神戸新交通六甲ライナー</prop>          <!-- 神戸新交通六甲アイランド線 -->
            <prop key="J004056">ＪＲきのくに線</prop>                  <!-- ＪＲ紀勢本線 -->
            <prop key="J002005">ＪＲ長野新幹線</prop>                  <!-- ＪＲ北陸新幹線 -->
            <prop key="P153001">リニモ</prop>                          <!-- 愛知高速交通リニモ -->
            <prop key="P149001">あおなみ線</prop>                      <!-- 名古屋臨海高速鉄道あおなみ線 -->
            <prop key="P145001">北鉄石川線</prop>                      <!-- 北陸鉄道石川線 -->
            <prop key="P145002">北鉄浅野川線</prop>                    <!-- 北陸鉄道浅野川線 -->
            <prop key="P071001">松電上高地線</prop>                    <!-- 松本電気鉄道上高地線 -->
            <prop key="P106001">長電長野線</prop>                      <!-- 長野電鉄長野線 -->
            <prop key="P106002">長電屋代線</prop>                      <!-- 長野電鉄屋代線 -->
            <prop key="P089001">静鉄静岡清水線</prop>                  <!-- 静岡鉄道静岡清水線 -->		  
          </props>
        </entry>
      </map>
    </property>
  </bean>
  
</beans>
