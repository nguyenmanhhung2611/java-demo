<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
                           http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.2.xsd">

	<!-- 物件特徴編集画面 -->
	<bean id="housingSpecialtyEditCommand"
		class="jp.co.transcosmos.dm3.webAdmin.housingSpecialty.command.HousingSpecialtyEditCommand"
		scope="prototype">
		<property name="panaHousingManager" ref="panaHousingManager" />
	</bean>

	<!-- 物件特徴編集確認画面 -->
	<bean id="housingSpecialtyConfirmCommand"
		class="jp.co.transcosmos.dm3.webAdmin.housingSpecialty.command.HousingSpecialtyConfirmCommand"
		scope="prototype">
	</bean>


	<!-- 物件特徴編集完了画面 -->
	<bean id="housingSpecialtyCompCommand"
		class="jp.co.transcosmos.dm3.webAdmin.housingSpecialty.command.HousingSpecialtyCompCommand"
		scope="prototype">
		<property name="panaHousingManager" ref="panaHousingManager" />
	</bean>

	<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMappingList">
		<property name="commandURLMapper" ref="commandURLMapper" />
		<property name="mappings">
			<list>

				<!-- 物件特徴編集画面 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/top/housing/part/input/" />
					<property name="defaultCommandName" value="housingSpecialtyEditCommand" />
					<property name="defaultViewName"
						value="/WEB-INF/jsp/admin/housingSpecialty/HousingSpecialtyUpdInput.jsp" />
				</bean>

				<!-- 物件特徴編集確認画面 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/top/housing/part/confirm/" />
					<property name="defaultCommandName" value="housingSpecialtyConfirmCommand" />
					<property name="defaultViewName"
						value="/WEB-INF/jsp/admin/housingSpecialty/HousingSpecialtyUpdConfirm.jsp" />
					<property name="namedViews">
						<props>
							<prop key="input">/WEB-INF/jsp/admin/housingSpecialty/HousingSpecialtyUpdInput.jsp
							</prop>
						</props>
					</property>
				</bean>


				<!-- 物件特徴編集完了画面 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/top/housing/part/result/" />
					<property name="defaultCommandName" value="housingSpecialtyCompCommand" />
					<property name="defaultViewName"
						value="/WEB-INF/jsp/admin/housingSpecialty/HousingSpecialtyCompRedirect.jsp" />
					<property name="namedViews">
						<props>
							<prop key="input">/WEB-INF/jsp/admin/housingSpecialty/HousingSpecialtyUpdInput.jsp
							</prop>
							<prop key="comp">/WEB-INF/jsp/admin/housingSpecialty/HousingSpecialtyComp.jsp
							</prop>
						</props>
					</property>
				</bean>

			</list>
		</property>
	</bean>
</beans>
