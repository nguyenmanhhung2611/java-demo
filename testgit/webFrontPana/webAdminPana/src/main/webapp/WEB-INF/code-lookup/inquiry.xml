<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

  <!-- お問合せ情報の定数管理 -->
  <bean class="jp.co.transcosmos.dm3.lookup.CodeLookupList">
    <property name="codeLookupManager"><ref bean="codeLookupManager"/></property>
    <property name="lookups">
      <map>
        <!-- 問合せ種別の定義 -->
        <entry key="inquiry_type">
          <list>
            <value>00:物件問合せ</value>
            <value>02:査定問合せ</value>
            <value>01:汎用問合せ</value>
          </list>
        </entry>
        <!-- お問合せ内容種別の定義 -->
        <entry key="inquiry_dtl_type">
          <list>
            <value>001:物件のご購入に関して</value>
            <value>002:リフォームに関して</value>
            <value>003:物件のご売却に関して</value>
            <value>004:セミナー・イベントに関して</value>
            <value>005:その他</value>
          </list>
        </entry>
        <!-- 物件お問合せ内容種別の定義 -->
        <entry key="inquiry_housing_dtl_type">
          <list>
            <value>001:物件を見たい</value>
            <value>002:物件の詳細情報がほしい</value>
            <value>003:ローン・購入など資金計画を相談したい</value>
            <value>004:希望物件に合う物件があれば紹介してほしい</value>
            <value>005:その他</value>
          </list>
        </entry>
        <!-- ステータスの定義 -->
        <entry key="inquiry_answerStatus">
          <list>
            <value>0:未対応</value>
            <value>1:開封済</value>
            <value>2:対応済</value>
          </list>
        </entry>
         <!-- 現状 -->
         <entry key="inquiry_present_cd">
           <list>
              <value>001:居住中</value>
              <value>002:賃貸中</value>
              <value>003:空家</value>
              <value>004:更地</value>
           </list>
        </entry>
         <!-- 売却予定時期 -->
         <entry key="inquiry_buy_time_cd">
           <list>
              <value>001:至急</value>
              <value>002:3カ月以内</value>
              <value>003:半年以内</value>
              <value>004:1年以内</value>
              <value>005:未定</value>
           </list>
        </entry>
        <!-- 連絡方法 -->
         <entry key="inquiry_contact_type">
           <list>
              <value>001:メール</value>
              <value>002:電話</value>
              <value>003:どちらでも可</value>
           </list>
        </entry>
        <!-- 連絡可能な時間帯 -->
         <entry key="inquiry_contact_time">
           <list>
              <value>001:午前（9:00～12:00）</value>
              <value>002:昼（12:00～13:00）</value>
              <value>003:午後（13:00～16:00）</value>
              <value>004:夕方（16:00～19:00）</value>
              <value>005:夜（19:00～21:00）</value>
           </list>
        </entry>
        <!-- 買い替えの有無 -->
        <entry key="replacement_flg">
          <list>
            <value>0:なし</value>
            <value>1:あり</value>
          </list>
        </entry>
        <!-- 売却物件種別 -->
        <entry key="buy_housing_type">
          <list>
            <value>01:マンション</value>
            <value>02:戸建</value>
            <value>03:土地</value>
          </list>
        </entry>
        <!-- アンケートの質問 -->
        <entry key="question">
          <list>
            <value>001:「ReaRie」のホームページをどこで知りましたか？（複数回答可）</value>
          </list>
        </entry>
        <!-- アンケートの回答（入力なし） -->
        <entry key="ans_etcNs">
          <list>
            <value>001:検索エンジン</value>
            <value>002:新聞広告</value>
            <value>003:雑誌広告</value>
            <value>004:チラシ</value>
            <value>005:知人の紹介</value>
            <value>006:クラブパナソニックの広告</value>
            <value>007:パナソニック社員／ＯＢである</value>
          </list>
        </entry>
        <!-- アンケートの回答（入力あり） -->
        <entry key="ans_etcAr">
          <list>
            <value>008:パナソニックショップ（ショップ名を記入してください）</value>
            <value>009:その他</value>
            <value>010:「プロモコード」があれば記入してください</value>
          </list>
        </entry>
        <!-- アンケートの回答（全て） -->
        <entry key="ans_all">
          <list>
            <value>001:検索エンジン</value>
            <value>002:新聞広告</value>
            <value>003:雑誌広告</value>
            <value>004:チラシ</value>
            <value>005:知人の紹介</value>
            <value>006:クラブパナソニックの広告</value>
            <value>007:パナソニック社員／ＯＢである</value>
            <value>008:パナソニックショップ（ショップ名を記入してください）</value>
            <value>009:その他</value>
            <value>010:「プロモコード」があれば記入してください</value>
          </list>
        </entry>
      </map>
    </property>
  </bean>

</beans>
