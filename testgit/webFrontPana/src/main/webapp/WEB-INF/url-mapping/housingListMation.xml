<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<!-- 画面初期化マンションのCommand -->
	<bean id="housingListMatinoInitCommandMansion"
		class="jp.co.transcosmos.dm3.webFront.housingListMation.command.HousingListMatinoInitCommand"
		scope="prototype">
		<property name="panaHousingManager" ref="panaHousingManager" />
		<property name="codeLookupManager" ref="codeLookupManager" />
		<property name="reformManager" ref="reformManager" />
		<property name="panaCommonManage" ref="panamCommonManager" />
		<property name="commonParameters" ref="commonParameters"/>
		<property name="buildingManager" ref="buildingManager" />
		<property name="panaFileUtil" ref="fileUtil" />
		<property name="visibleNavigationPageCount" value="4"/>

		<property name="rowsPerPage" value="15"/>
		<property name="housingKindCd" value="01" />
	</bean>

	<!-- 画面初期化戸建のCommand -->
	<bean id="housingListMatinoInitCommandHouse"
		class="jp.co.transcosmos.dm3.webFront.housingListMation.command.HousingListMatinoInitCommand"
		scope="prototype">
		<property name="panaHousingManager" ref="panaHousingManager" />
		<property name="codeLookupManager" ref="codeLookupManager" />
		<property name="reformManager" ref="reformManager" />
		<property name="panaCommonManage" ref="panamCommonManager" />
		<property name="commonParameters" ref="commonParameters"/>
		<property name="panaFileUtil" ref="fileUtil" />
		<property name="buildingManager" ref="buildingManager" />

		<property name="rowsPerPage" value="15"/>
		<property name="housingKindCd" value="02" />
	</bean>

	<!-- 画面初期化土地のCommand -->
	<bean id="housingListMatinoInitCommandGround"
		class="jp.co.transcosmos.dm3.webFront.housingListMation.command.HousingListMatinoInitCommand"
		scope="prototype">
		<property name="panaHousingManager" ref="panaHousingManager" />
		<property name="codeLookupManager" ref="codeLookupManager" />
		<property name="reformManager" ref="reformManager" />
		<property name="panaCommonManage" ref="panamCommonManager" />
		<property name="commonParameters" ref="commonParameters"/>
		<property name="buildingManager" ref="buildingManager" />
		<property name="panaFileUtil" ref="fileUtil" />

		<property name="rowsPerPage" value="15"/>
		<property name="housingKindCd" value="03" />
	</bean>

	<!-- 画面検索マンションのCommand -->
	<bean id="housingListSearchCommandMansion"
		class="jp.co.transcosmos.dm3.webFront.housingListMation.command.HousingListMatinoSearchCommand"
		scope="prototype">
		<property name="codeLookupManager" ref="codeLookupManager" />
		<property name="panaHousingManager" ref="panaHousingManager" />
		<property name="housingKindCd" value="01" />
	</bean>

	<!-- 画面検索戸建のCommand -->
	<bean id="housingListSearchCommandHouse"
		class="jp.co.transcosmos.dm3.webFront.housingListMation.command.HousingListMatinoSearchCommand"
		scope="prototype">
		<property name="codeLookupManager" ref="codeLookupManager" />
		<property name="panaHousingManager" ref="panaHousingManager" />
		<property name="housingKindCd" value="02" />
	</bean>

	<!-- 画面検索土地のCommand -->
	<bean id="housingListSearchCommandGround"
		class="jp.co.transcosmos.dm3.webFront.housingListMation.command.HousingListMatinoSearchCommand"
		scope="prototype">
		<property name="codeLookupManager" ref="codeLookupManager" />
		<property name="panaHousingManager" ref="panaHousingManager" />
		<property name="housingKindCd" value="03" />
	</bean>


	<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMappingList">
		<property name="commandURLMapper" ref="commandURLMapper" />
		<property name="mappings">
			<list>

				<!-- 画面初期化マンション -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/buy/mansion/###prefCd###/area/###addressCd###/" />
					<property name="defaultCommandName" value="housingListMatinoInitCommandMansion" />
					<property name="defaultViewName"
						value="/WEB-INF/jsp/front/housingListMation/HousingListMationInit.jsp" />
				</bean>

				<!-- 画面初期化戸建 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/buy/house/###prefCd###/area/###addressCd###/" />
					<property name="defaultCommandName" value="housingListMatinoInitCommandHouse" />
					<property name="defaultViewName"
						value="/WEB-INF/jsp/front/housingListMation/HousingListMationInit.jsp" />
				</bean>

				<!-- 画面初期化土地 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/buy/ground/###prefCd###/area/###addressCd###/" />
					<property name="defaultCommandName" value="housingListMatinoInitCommandGround" />
					<property name="defaultViewName"
						value="/WEB-INF/jsp/front/housingListMation/HousingListMationInit.jsp" />
				</bean>

				<!-- 画面初期化マンション -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/buy/mansion/###prefCd###/route/###routeCd###/" />
					<property name="defaultCommandName" value="housingListMatinoInitCommandMansion" />
					<property name="defaultViewName"
						value="/WEB-INF/jsp/front/housingListMation/HousingListMationInit.jsp" />
				</bean>

				<!-- 画面初期化戸建 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/buy/house/###prefCd###/route/###routeCd###/" />
					<property name="defaultCommandName" value="housingListMatinoInitCommandHouse" />
					<property name="defaultViewName"
						value="/WEB-INF/jsp/front/housingListMation/HousingListMationInit.jsp" />
				</bean>

				<!-- 画面初期化土地 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/buy/ground/###prefCd###/route/###routeCd###/" />
					<property name="defaultCommandName" value="housingListMatinoInitCommandGround" />
					<property name="defaultViewName"
						value="/WEB-INF/jsp/front/housingListMation/HousingListMationInit.jsp" />
				</bean>

				<!-- 画面初期化マンション -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/buy/mansion/###prefCd###/route/###routeCd###/station/###stationCd###/" />
					<property name="defaultCommandName" value="housingListMatinoInitCommandMansion" />
					<property name="defaultViewName"
						value="/WEB-INF/jsp/front/housingListMation/HousingListMationInit.jsp" />
				</bean>

				<!-- 画面初期化戸建 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/buy/house/###prefCd###/route/###routeCd###/station/###stationCd###/" />
					<property name="defaultCommandName" value="housingListMatinoInitCommandHouse" />
					<property name="defaultViewName"
						value="/WEB-INF/jsp/front/housingListMation/HousingListMationInit.jsp" />
				</bean>

				<!-- 画面初期化土地 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/buy/ground/###prefCd###/route/###routeCd###/station/###stationCd###/" />
					<property name="defaultCommandName" value="housingListMatinoInitCommandGround" />
					<property name="defaultViewName"
						value="/WEB-INF/jsp/front/housingListMation/HousingListMationInit.jsp" />
				</bean>

				<!-- 画面初期化マンション -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/buy/mansion/###prefCd###/list/" />
					<property name="defaultCommandName" value="housingListMatinoInitCommandMansion" />
					<property name="defaultViewName"
						value="/WEB-INF/jsp/front/housingListMation/HousingListMationInit.jsp" />
				</bean>

				<!-- 画面初期化戸建 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/buy/house/###prefCd###/list/" />
					<property name="defaultCommandName" value="housingListMatinoInitCommandHouse" />
					<property name="defaultViewName"
						value="/WEB-INF/jsp/front/housingListMation/HousingListMationInit.jsp" />
				</bean>

				<!-- 画面初期化土地 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/buy/ground/###prefCd###/list/" />
					<property name="defaultCommandName" value="housingListMatinoInitCommandGround" />
					<property name="defaultViewName"
						value="/WEB-INF/jsp/front/housingListMation/HousingListMationInit.jsp" />
				</bean>

				<!-- 画面初期化マンション -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/buy/mansion/###prefCd###/searchCount/" />
					<property name="defaultCommandName" value="housingListSearchCommandMansion" />
					<property name="defaultViewName"
						value="json" />
				</bean>

				<!-- 画面初期化戸建 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/buy/house/###prefCd###/searchCount/" />
					<property name="defaultCommandName" value="housingListSearchCommandHouse" />
					<property name="defaultViewName"
						value="json" />
				</bean>

				<!-- 画面初期化土地 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/buy/ground/###prefCd###/searchCount/" />
					<property name="defaultCommandName" value="housingListSearchCommandGround" />
					<property name="defaultViewName"
						value="json" />
				</bean>
			</list>
		</property>
	</bean>
</beans>
