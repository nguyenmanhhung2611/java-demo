<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

  <bean class="jp.co.transcosmos.dm3.lookup.CodeLookupList">
    <property name="codeLookupManager"><ref bean="codeLookupManager"/></property>
    <property name="lookups">

	  <!-- 権限情報の定数管理 -->
      <map>
        <entry key="roleId">
          <list>
            <value>:</value>
            <value>admin:管理者</value>
            <value>user:一般ユーザー</value>
          </list>
        </entry>
        <entry key="radio_roleId">
          <list>
            <value>user:通常権限</value>
            <value>admin:管理権限（個人情報アクセス権限あり）</value>
          </list>
        </entry>
      </map>
    </property>
  </bean>

</beans>
