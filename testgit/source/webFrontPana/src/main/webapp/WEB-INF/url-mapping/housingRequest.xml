<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<!-- 画面初期化 -->
	<bean id="housingRequestInitCommand"
		class="jp.co.transcosmos.dm3.webFront.housingRequest.command.HousingRequestInitCommand"
		scope="prototype">
		<property name="codeLookupManager" ref="codeLookupManager" />
		<property name="panaCommonManage" ref="panamCommonManager" />
		<property name="housingRequestManage" ref="housingRequestManage" />
	</bean>

	<!-- 画面確認 -->
	<bean id="housingRequestConfirmCommand"
		class="jp.co.transcosmos.dm3.webFront.housingRequest.command.HousingRequestConfirmCommand"
		scope="prototype">
		<property name="codeLookupManager" ref="codeLookupManager" />
		<property name="panaCommonManage" ref="panamCommonManager" />
		<property name="commonParameters" ref="commonParameters" />
		<property name="housingRequestManage" ref="housingRequestManage" />
	</bean>

	<!-- 画面完了 -->
	<bean id="housingRequestCompCommand"
		class="jp.co.transcosmos.dm3.webFront.housingRequest.command.HousingRequestCompCommand"
		scope="prototype">
		<property name="codeLookupManager" ref="codeLookupManager" />
		<property name="housingRequestManage" ref="housingRequestManage" />
		<property name="panaCommonManage" ref="panamCommonManager" />
		<property name="commonParameters" ref="commonParameters" />
		<property name="sendRequestInsertMansion" ref="sendRequestInsertMansion" />
		<property name="sendRequestInsertHouse" ref="sendRequestInsertHouse" />
		<property name="sendRequestInsertLand" ref="sendRequestInsertLand" />
		<property name="sendRequestUpdateMansion" ref="sendRequestUpdateMansion" />
		<property name="sendRequestUpdateHouse" ref="sendRequestUpdateHouse" />
		<property name="sendRequestUpdateLand" ref="sendRequestUpdateLand" />
	</bean>

	<!-- 画面削除確認 -->
	<bean id="housingRequestDeleteCommand"
		class="jp.co.transcosmos.dm3.webFront.housingRequest.command.HousingRequestDeleteCommand"
		scope="prototype">
		<property name="codeLookupManager" ref="codeLookupManager" />
		<property name="panaCommonManage" ref="panamCommonManager" />
		<property name="housingRequestManage" ref="housingRequestManage" />
	</bean>

	<!-- 画面削除完了 -->
	<bean id="housingRequestDeleteCompCommand"
		class="jp.co.transcosmos.dm3.webFront.housingRequest.command.HousingRequestDeleteCompCommand"
		scope="prototype">
		<property name="codeLookupManager" ref="codeLookupManager" />
		<property name="housingRequestManage" ref="housingRequestManage" />
		<property name="panaCommonManage" ref="panamCommonManager" />
		<property name="commonParameters" ref="commonParameters" />
		<property name="sendRequestDeleteMansion" ref="sendRequestDeleteMansion" />
		<property name="sendRequestDeleteHouse" ref="sendRequestDeleteHouse" />
		<property name="sendRequestDeleteLand" ref="sendRequestDeleteLand" />
	</bean>
	
	<bean id="housingRouteCommand"
		class="jp.co.transcosmos.dm3.webFront.housingRequest.command.RouteMstListCommand"
		scope="prototype">
		
		<property name="panaCommonManager" ref="panamCommonManager" />
	</bean>
	
	

	<!-- 画面初期化 -->
	<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMappingList">
		<property name="commandURLMapper" ref="commandURLMapper" />
		<property name="mappings">
			<list>	
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/mypage/request/routeMstList/" />
					 <property name="defaultCommandName" value="housingRequestInitCommand" />
					<property name="defaultViewName" value ="json"/>
				</bean>
				
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/mypage/request/stationMstList/" />
					 <property name="defaultCommandName" value="housingRequestInitCommand" />
					<property name="defaultViewName" value ="json"/>
				</bean>
				
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/mypage/request/input/" />
					<property name="defaultCommandName" value="housingRequestInitCommand" />
					<property name="defaultViewName"
						value="/WEB-INF/jsp/front/housingRequest/HousingRequestInit.jsp" />
				</bean>

				<!-- 画面確認 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/mypage/request/confirm/" />
					<property name="defaultCommandName" value="housingRequestConfirmCommand" />
					<property name="defaultViewName"
						value="/WEB-INF/jsp/front/housingRequest/HousingRequestConfirm.jsp" />
					<property name="namedViews">
						<props>
							<prop key="validFail">/WEB-INF/jsp/front/housingRequest/HousingRequestInit.jsp
							</prop>
							<prop key="404">/WEB-INF/jsp/front/common/404.jsp
							</prop>
						</props>
					</property>

				</bean>

				<!-- CSRF 対策用 token のチェックフィルター -->
				<!-- フィルターの実行タイミングが固定できないので、global.xml へ移動 <bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping"> 
					<property name="urlPattern" value="/mypage/request/result/" /> <property 
					name="defaultCommandName" value="csrfChkFilter"/> <property name="filter" 
					value="true"/> </bean> -->

				<!-- 画面完了 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/mypage/request/result/" />
					<property name="defaultCommandName" value="housingRequestCompCommand" />
					<property name="defaultViewName"
						value="/WEB-INF/jsp/front/housingRequest/HousingRequestCompRedirect.jsp" />
					<property name="namedViews">
						<props>
							<prop key="validFail">/WEB-INF/jsp/front/housingRequest/HousingRequestInit.jsp
							</prop>
							<prop key="comp">/WEB-INF/jsp/front/housingRequest/HousingRequestComp.jsp
							</prop>
						</props>
					</property>

				</bean>
				
				<!-- 画面削除 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/mypage/request/delete/confirm/" />
					<property name="defaultCommandName" value="housingRequestDeleteCommand" />
					<property name="defaultViewName"
						value="/WEB-INF/jsp/front/housingRequest/HousingRequestDeleteConfirm.jsp" />
					<property name="namedViews">
						<props>
							<prop key="validFail">/WEB-INF/jsp/front/housingRequest/HousingRequestInit.jsp
							</prop>
							<prop key="404">/WEB-INF/jsp/front/common/404.jsp
							</prop>
						</props>
					</property>

				</bean>

				<!-- CSRF 対策用 token のチェックフィルター -->
				<!-- フィルターの実行タイミングが固定できないので、global.xml へ移動 <bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping"> 
					<property name="urlPattern" value="/mypage/request/delete/result/" /> <property 
					name="defaultCommandName" value="csrfChkFilter"/> <property name="filter" 
					value="true"/> </bean> -->

				<!-- 画面削除完了 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/mypage/request/delete/result/" />
					<property name="defaultCommandName" value="housingRequestDeleteCompCommand" />
					<property name="defaultViewName"
						value="/WEB-INF/jsp/front/housingRequest/HousingRequestDeleteCompRedirect.jsp" />
					<property name="namedViews">
						<props>
							<prop key="validFail">/WEB-INF/jsp/front/housingRequest/HousingRequestInit.jsp
							</prop>
							<prop key="comp">/WEB-INF/jsp/front/housingRequest/HousingRequestDeleteComp.jsp
							</prop>
						</props>
					</property>

				</bean>

				
			</list>
		</property>
	</bean>



</beans>
