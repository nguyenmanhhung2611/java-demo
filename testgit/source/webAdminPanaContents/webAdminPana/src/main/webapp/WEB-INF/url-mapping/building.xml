<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
                           http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.2.xsd">

	<!-- 建物情報メンテナンスの検索・一覧画面初期化 -->
	<bean id="buildingListInitCommand"
		class="jp.co.transcosmos.dm3.adminCore.building.command.BuildingListInitCommand"
		scope="prototype">
		<property name="prefMstDAO" ref="prefMstDAO" />
	</bean>

	<!-- 建物情報メンテナンスの検索・一覧画面 -->
	<bean id="buildingListCommand"
		class="jp.co.transcosmos.dm3.adminCore.building.command.BuildingListCommand"
		scope="prototype">
		<property name="buildingManager" ref="buildingManager" />
		<property name="prefMstDAO" ref="prefMstDAO" />
		<property name="rowsPerPage" value="50" />
	</bean>

	<!-- 建物基本情報新規登録入力画面 -->
	<bean id="buildingAddInputCommand"
		class="jp.co.transcosmos.dm3.adminCore.building.command.BuildingInputCommand"
		scope="prototype">
		<property name="mode" value="insert" />
		<property name="prefMstDAO" ref="prefMstDAO" />
		<property name="addressMstDAO" ref="addressMstDAO" />
	</bean>

	<!-- 建物基本情報新規登録入力確認画面 -->
	<bean id="buildingAddConfirmCommand"
		class="jp.co.transcosmos.dm3.adminCore.building.command.BuildingConfirmCommand"
		scope="prototype">
		<property name="mode" value="insert" />
		<property name="prefMstDAO" ref="prefMstDAO" />
		<property name="addressMstDAO" ref="addressMstDAO" />
	</bean>

	<!-- 建物基本情報新規登録完了画面 -->
	<bean id="buildingAddCompCommand"
		class="jp.co.transcosmos.dm3.adminCore.building.command.BuildingCompCommand"
		scope="prototype">
		<property name="buildingManager" ref="buildingManager" />
		<property name="mode" value="insert" />
		<property name="useValidation" value="false" />
	</bean>

	<!-- 建物基本情報新規変更入力画面 -->
	<bean id="buildingUpdInputCommand"
		class="jp.co.transcosmos.dm3.adminCore.building.command.BuildingInputCommand"
		scope="prototype">
		<property name="mode" value="update" />
		<property name="prefMstDAO" ref="prefMstDAO" />
		<property name="addressMstDAO" ref="addressMstDAO" />
		<property name="buildingManager" ref="buildingManager" />
	</bean>
	<!-- 建物基本情報変更確認画面 -->
	<bean id="buildingUpdConfirmCommand"
		class="jp.co.transcosmos.dm3.adminCore.building.command.BuildingConfirmCommand"
		scope="prototype">
		<property name="mode" value="update" />
		<property name="prefMstDAO" ref="prefMstDAO" />
		<property name="addressMstDAO" ref="addressMstDAO" />
	</bean>

	<!-- 建物基本情報変更登録完了画面 -->
	<bean id="buildingUpdCompCommand"
		class="jp.co.transcosmos.dm3.adminCore.building.command.BuildingCompCommand"
		scope="prototype">
		<property name="buildingManager" ref="buildingManager" />
		<property name="mode" value="update" />
		<property name="prefMstDAO" ref="prefMstDAO" />
		<property name="addressMstDAO" ref="addressMstDAO" />
	</bean>

	<!-- 建物閲覧画面 -->
	<bean id="buildingDetailCommand"
		class="jp.co.transcosmos.dm3.adminCore.building.command.BuildingDetailCommand"
		scope="prototype">
		<property name="buildingManager" ref="buildingManager" />
	</bean>


	<!-- 市区町村プルダウンリスト取得 -->
	<bean id="addressMstListCommand"
		class="jp.co.transcosmos.dm3.core.command.AddressMstListCommand"
		scope="prototype">
		<property name="addressMstDAO" ref="addressMstDAO" />
	</bean>

	<!-- 最寄り駅情報新規変更入力画面 -->
	<bean id="buildingStationInfoUpdInputCommand"
		class="jp.co.transcosmos.dm3.adminCore.building.command.BuildingStationInfoInputCommand"
		scope="prototype">
		<property name="mode" value="update" />
		<property name="routeMstListDAO" ref="routeMstListDAO" />
		<property name="stationMstListDAO" ref="stationMstListDAO" />
		<property name="buildingManager" ref="buildingManager" />
	</bean>

	<!-- 最寄り駅情報変更確認画面 -->
	<bean id="buildingStationInfoUpdConfirmCommand"
		class="jp.co.transcosmos.dm3.adminCore.building.command.BuildingStationInfoConfirmCommand"
		scope="prototype">
		<property name="mode" value="update" />
		<property name="routeMstListDAO" ref="routeMstListDAO" />
		<property name="stationMstListDAO" ref="stationMstListDAO" />
	</bean>

	<!-- 駅プルダウンリスト取得 -->
	<bean id="stationMstListCommand"
		class="jp.co.transcosmos.dm3.core.command.StationMstListCommand"
		scope="prototype">
		<property name="stationMstListDAO" ref="stationMstListDAO" />
	</bean>

	<!-- 最寄り駅情報変更登録完了画面 -->
	<bean id="buildingStationInfoUpdCompCommand"
		class="jp.co.transcosmos.dm3.adminCore.building.command.BuildingStationInfoCompCommand"
		scope="prototype">
		<property name="buildingManager" ref="buildingManager" />
		<property name="mode" value="update" />
		<property name="useValidation" value="false" />
		<property name="routeMstListDAO" ref="routeMstListDAO" />
		<property name="stationMstListDAO" ref="stationMstListDAO" />
	</bean>
	<!-- 建物情報削除確認画面 -->
	<bean id="buildingDelConfirmCommand"
		class="jp.co.transcosmos.dm3.adminCore.building.command.BuildingConfirmCommand"
		scope="prototype">
		<property name="buildingManager" ref="buildingManager" />
		<property name="mode" value="delete" />
	</bean>
	<!-- 建物情報削除完了画面 -->
	<bean id="buildingDelCompCommand"
		class="jp.co.transcosmos.dm3.adminCore.building.command.BuildingCompCommand"
		scope="prototype">
		<property name="buildingManager" ref="buildingManager" />
		<property name="mode" value="delete" />
	</bean>

	<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMappingList">
		<property name="commandURLMapper" ref="commandURLMapper" />
		<property name="mappings">
			<list>
				<!-- 建物情報の検索・一覧画面 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/top/building/list" />
					<property name="defaultCommandName" value="buildingListInitCommand" />
					<property name="namedCommands">
						<props>
							<prop key="list">buildingListCommand</prop>
						</props>
					</property>
					<property name="defaultViewName"
						value="/WEB-INF/admin/default_jsp/building/BuildingList.jsp" />
				</bean>
				<!-- 建物情報新規入力画面 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/top/building/addInput" />
					<property name="defaultCommandName" value="buildingAddInputCommand" />
					<property name="defaultViewName"
						value="/WEB-INF/admin/default_jsp/building/BuildingAddInput.jsp" />
				</bean>
				<!-- 建物情報新規入力確認画面 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/top/building/addConfirm" />
					<property name="defaultCommandName" value="buildingAddConfirmCommand" />
					<property name="defaultViewName"
						value="/WEB-INF/admin/default_jsp/building/BuildingAddConfirm.jsp" />
					<property name="namedViews">
						<props>
							<prop key="input">/WEB-INF/admin/default_jsp/building/BuildingAddInput.jsp
							</prop>
						</props>
					</property>
				</bean>
				<!-- 建物情報新規登録完了画面 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/top/building/insert" />
					<property name="defaultCommandName" value="buildingAddCompCommand" />
					<property name="defaultViewName"
						value="/WEB-INF/admin/default_jsp/building/BuildingCompRedirect.jsp" />
					<property name="namedViews">
						<props>
							<prop key="input">/WEB-INF/admin/default_jsp/building/BuildingAddInput.jsp
							</prop>
							<prop key="comp">/WEB-INF/admin/default_jsp/building/BuildingComp.jsp
							</prop>
						</props>
					</property>
				</bean>

				<!-- 建物情報変更入力画面 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/top/building/updInput" />
					<property name="defaultCommandName" value="buildingUpdInputCommand" />
					<property name="defaultViewName"
						value="/WEB-INF/admin/default_jsp/building/BuildingUpdInput.jsp" />
				</bean>
				<!-- 建物情報変更入力確認画面 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/top/building/updConfirm" />
					<property name="defaultCommandName" value="buildingUpdConfirmCommand" />
					<property name="defaultViewName"
						value="/WEB-INF/admin/default_jsp/building/BuildingUpdConfirm.jsp" />
					<property name="namedViews">
						<props>
							<prop key="input">/WEB-INF/admin/default_jsp/building/BuildingUpdInput.jsp
							</prop>
						</props>
					</property>
				</bean>
				<!-- 建物情報変更登録完了画面 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/top/building/update" />
					<property name="defaultCommandName" value="buildingUpdCompCommand" />
					<property name="defaultViewName"
						value="/WEB-INF/admin/default_jsp/building/BuildingCompRedirect.jsp" />
					<property name="namedViews">
						<props>
							<prop key="input">/WEB-INF/admin/default_jsp/building/BuildingUpdInput.jsp
							</prop>
							<prop key="comp">/WEB-INF/admin/default_jsp/building/BuildingComp.jsp
							</prop>
						</props>
					</property>
				</bean>
		        <!-- 建物閲覧画面 -->
		        <bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
		          <property name="urlPattern" value="/top/building/detail"/>
		          <property name="defaultCommandName" value="buildingDetailCommand"/>
		          <property name="defaultViewName" value="/WEB-INF/admin/default_jsp/building/BuildingDetail.jsp"/>
		        </bean>
				<!-- 市区町村プルダウンリスト取得 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/top/building/addressMstList" />
					<property name="defaultCommandName" value="addressMstListCommand" />
					<property name="defaultViewName" value="json" />
				</bean>
				<!-- 最寄り駅情報変更入力画面 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/top/building/updBuildingStationInfoInput" />
					<property name="defaultCommandName" value="buildingStationInfoUpdInputCommand" />
					<property name="defaultViewName"
						value="/WEB-INF/admin/default_jsp/building/BuildingStationInfoUpdInput.jsp" />
				</bean>
				<!-- 駅プルダウンリスト取得 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/top/building/stationMstList" />
					<property name="defaultCommandName" value="stationMstListCommand" />
					<property name="defaultViewName" value="json" />
				</bean>
				<!-- 最寄り駅情報変更入力確認画面 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/top/building/updBuildingStationInfoConfirm" />
					<property name="defaultCommandName" value="buildingStationInfoUpdConfirmCommand" />
					<property name="defaultViewName"
						value="/WEB-INF/admin/default_jsp/building/BuildingStationInfoUpdConfirm.jsp" />
					<property name="namedViews">
						<props>
							<prop key="input">/WEB-INF/admin/default_jsp/building/BuildingStationInfoUpdInput.jsp
							</prop>
						</props>
					</property>
				</bean>
				<!-- 最寄り駅変更登録完了画面 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/top/building/updBuildingStationInfo" />
					<property name="defaultCommandName" value="buildingStationInfoUpdCompCommand" />
					<property name="defaultViewName"
						value="/WEB-INF/admin/default_jsp/building/BuildingStationInfoCompRedirect.jsp" />
					<property name="namedViews">
						<props>
							<prop key="input">/WEB-INF/admin/default_jsp/building/BuildingStationInfoUpdInput.jsp
							</prop>
							<prop key="comp">/WEB-INF/admin/default_jsp/building/BuildingStationInfoComp.jsp
							</prop>
						</props>
					</property>
				</bean>
		        <!-- 建物削除確認画面 -->
		        <bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
		          <property name="urlPattern" value="/top/building/delete"/>
		          <property name="defaultCommandName" value="buildingDelConfirmCommand"/>
		          <property name="defaultViewName" value="/WEB-INF/admin/default_jsp/building/BuildingDelConfirm.jsp"/>
		        </bean>
		        <!-- 建物情報削除完了画面 -->
				<bean class="jp.co.transcosmos.dm3.servlet.URLCommandViewMapping">
					<property name="urlPattern" value="/top/building/deleteBuilding" />
					<property name="defaultCommandName" value="buildingDelCompCommand" />
					<property name="defaultViewName"
						value="/WEB-INF/admin/default_jsp/building/BuildingCompRedirect.jsp" />
					<property name="namedViews">
						<props>
							<prop key="input">/WEB-INF/admin/default_jsp/building/BuildingList.jsp
							</prop>
							<prop key="comp">/WEB-INF/admin/default_jsp/building/BuildingDelComp.jsp
							</prop>
						</props>
					</property>
				</bean>
			</list>
		</property>
	</bean>

</beans>
